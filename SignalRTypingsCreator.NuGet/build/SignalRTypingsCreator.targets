<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="CreateSinglRTypings" AfterTargets="Build">
    <Message Importance="high" Text="Creating typings from assembly '$(AssemblyName)'" />
    <Exec Command='"$(MSBuildProjectDirectory)\$(OutDir)SignalRTypingsCreator.exe" "$(AssemblyName)" "$(MSBuildProjectDirectory)" "$(MSBuildProjectFullPath)"' />
  </Target>
</Project>